{
  "name": "blog.scottspence.me",
  "version": "3.0.0",
  "main": "index.js",
  "author": "Scott Spence <spences10apps@gmail.com>(https://blog.scottspence.me/)",
  "license": "MIT",
  "description": "Personal blog made with React Static",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build && mv dist scottblog",
    "serve": "serve scottblog -p 4000",
    "clean": "rm -rf scottblog/ && rm -rf dist/",
    "deploy:now": "npm run clean && npm run build && now scottblog/ --dotenv",
    "format": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "deploy": "now scottblog/ --token $NOW_TOKEN",
    "alias:dev": "now alias blog-scottspence.now.sh --token $NOW_TOKEN",
    "alias:prod": "now alias blog.scottspence.me --token $NOW_TOKEN",
    "cleanup": "now rm scottblog --safe --yes --token $NOW_TOKEN"
  },
  "keywords": [
    "react-static",
    "personal",
    "blog"
  ],
  "now": {
    "name": "scottblog"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "date-fns": "1.29.0",
    "dotenv": "6.0.0",
    "graphql-request": "1.6.0",
    "prismjs": "1.14.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "5.2.0",
    "react-markdown": "3.3.3",
    "react-router": "^4.2.0",
    "react-smackdown": "1.0.0-beta.3",
    "react-static": "5.9.7",
    "styled-components": "^3"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.3.0",
    "eslint-config-react-tools": "1.x.x",
    "serve": "^6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/blog.scottspence.me.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/blog.scottspence.me/issues"
  },
  "homepage": "https://github.com/spences10/blog.scottspence.me#readme"
}
